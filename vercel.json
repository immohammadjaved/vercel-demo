{
  "version": 2,
  "builds": [
    {
      "src": "dist/ngxpress/server/server.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/**",
          "dist/ngxpress/server/**",
          "dist/ngxpress/browser/**",
          "src/assets/**",
          "*.db"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/ngxpress/server/server.mjs"
    }
  ]
} 