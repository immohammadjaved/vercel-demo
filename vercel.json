{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/ngxpress",
  "installCommand": "npm install",
  "framework": "angular",
  "functions": {
    "dist/ngxpress/server/server.mjs": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/ngxpress/server/server.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 